{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "ES2016",
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "outDir": "./dist"
  },
  "include": ["./src"],
  "exclude": ["./dist", "node_modules"]
}
